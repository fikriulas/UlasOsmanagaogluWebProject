@if (Model.Twitter != "" && Model.Twitter != null)
{
    <li><a href="@Model.Twitter" target="popup"><span class="fa fa-twitter"></span> Twitter</a></li>
}
@if (Model.Facebook != "" && Model.Facebook != null)
{
    <li><a href="@Model.Facebook" target="popup"><span class="fa fa-facebook"></span> Facebook</a></li>

}
@if (Model.Instagram != "" && Model.Instagram != null)
{
    <li><a href="@Model.Instagram" target="popup"><span class="fa fa-instagram"></span> Instagram</a></li>

